{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
  "baseUrl": ".",
  "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./*"
      ]
  }
  },
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    "src/**/*.ts",
    "src/**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "global.d.ts"
  ]
  ,
  "exclude": ["node_modules", "dist"]
}